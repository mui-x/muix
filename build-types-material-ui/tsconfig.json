{
  "compilerOptions": {
    "target": "es6",
    "moduleResolution": "node",
    //"esModuleInterop": true,

    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noImplicitReturns": true,
    "strictFunctionTypes": false,
    "noImplicitAny": false,
    "jsx": "react",
    "lib": [
      "dom",
      "es2015",
      "esnext.asynciterable"
    ]
  },
  "include": [
    "**/*",
    "node_modules/popper.js/index.d.ts"
  ],

  "exclude": [
    //"node_modules",
    //"material-ui",
    "typescript.d.ts"
  ]
}
