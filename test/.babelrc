{
  "presets": [ "babel-preset-expo" ],
  "env": {
    "development": {
      "plugins": [ "transform-react-jsx-source" ]
    }
  },
  "plugins": [
    [
      "module-resolver",
      {
        "root": [ "." ],
        "alias": {
          "^muix-icons/(.+)": "muix-icons/es",
          "muix-components": "muix-components/es/native"
        }
      }
    ]
  ]
}
