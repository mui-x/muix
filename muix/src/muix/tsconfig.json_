{
  "extends": "../tsconfig-web.common.json",
  "compilerOptions": {
    "outDir": "../../deploy/lib/muix",
    "strict": false,
    "baseUrl": ".",
    "paths": {
      "reactxx-sheeter": [
        "../sheeter"
      ],
      "reactxx-fela": [
        "../fela"
      ],
      "reactxx-basic": [
        "../basic"
      ],
      "reactxx-mui-web/*": [
        "../mui-web/*"
      ],
      "reactxx-icons/*": [
        "../icons/*"
      ],
    }
  },
  "references": [
    {
      "path": "../fela"
    },
    {
      "path": "../sheeter"
    },
    {
      "path": "../basic"
    },
    {
      "path": "../mui-web"
    },
    {
      "path": "../icons"
    },
  ],
  "exclude": [
    "./reactxx-mui-web/**/*"
  ]
}