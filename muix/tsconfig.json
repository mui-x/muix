{
  "compileOnSave": true,
  "compilerOptions": {
    //"watch": true,
    "target": "es5",
    "module": "system",
    "moduleResolution": "node",
    "allowJs": false,
    //"declaration": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "rootDir": "src",
    "outDir": "deploy/lib",
    "jsx": "react",
    //"jsxFactory": "Reactxx.createElement",
    "inlineSourceMap": true,
    "inlineSources": true,
    "importHelpers": true,
    "strictNullChecks": false,
    "downlevelIteration": true,
    "strict": false,
    "strictFunctionTypes": false,
    "noImplicitAny": false,
    "removeComments": true,
    //"noUnusedLocals": true,
    "baseUrl": ".",
    "paths": {
      "reactxx-sheeter": [
        "src/sheeter/index"
      ],
      "reactxx-fela": [
        "src/fela/index"
      ],
      "reactxx-animation": [
        "src/animation/index"
      ],
      "reactxx-basic": [
        "src/basic/index"
      ],
      "reactxx-primitives": [
        "src/primitives/web/index"
      ],
      "reactxx-mediaq": [
        "src/mediaq/web/web"
      ],
      "reactxx-icons/*": [
        "src/icons/*"
      ],
      "reactxx-mui-web/*": [
        "src/mui-web/*"
      ],
      "reactxx-muix/*": [
        "src/muix/*"
      ]
    },
    "lib": [
      "dom",
      "es2017",
      "esnext.asynciterable"
    ]
  },
  "include": [
    //"src/**/*",
    "src/typings.d.ts",
    "src/sheeter/**/*",
    "src/fela/**/*",
    "src/basic/**/*",
    "src/primitives/**/*",
    // "src/media/**/*",
    // "src/animation/**/*",
    // "src/mui-web/**/*",
    // "src/icons/**/*",
    // "src/muix/**/*",
    // "src/ks/common/muix-doc/**/*",
    // "src/ks/common/mui-doc-app/**/*",
    // "src/ks/web/**/*",
  ],
  "exclude": [
    "src/*/typings.d.ts",
    "src/trash",
    "src/ks/common/muix",
    "src/ks/native",
    "src/icons/native",
    "src/test",
    //"src/muix/reactxx-mui-web"
    "src/muix/reactxx-mui-web"
  ]
}