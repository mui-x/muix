{
  "compileOnSave": true,
  "compilerOptions": {
    //"watch": true,
    "target": "es5",
    "module": "system",
    "moduleResolution": "node",
    "rootDir": "src",
    "outDir": "deploy/lib",
    "declaration": true,
    "allowJs": false,
    //"outFile": "all.js",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "inlineSourceMap": true,
    //"sourceMap": false,
    //"inlineSources": true,
    "importHelpers": true,
    "strictNullChecks": false,
    "downlevelIteration": true,
    "strict": false,
    "strictFunctionTypes": false,
    "noImplicitAny": false,
    "removeComments": true,
    
    //"noUnusedLocals": true,
    "baseUrl": ".",
    "paths": {
      "reactxx-typings": [
        "src/typings/index"
      ],
      "reactxx-fela": [
