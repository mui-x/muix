{
  "presets": [ "babel-preset-expo" ],
  "env": {
    "development": {
      "plugins": [ "transform-react-jsx-source" ]
    }
  },
  "plugins": [
    [
      "module-resolver",
      {
        "root": [ "." ],
        "alias": {
          "^reactxx-mdi/(.+)": "reactxx-mdi/es",

          "reactxx": "./deploy/es/reactxx/native",
          "reactxx-mui": "./deploy/es/mui/native",
          "reactxx-stateman": "./deploy/es/stateman"
        }
      }
    ]
  ]
}
