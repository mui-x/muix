{
  "presets": [ "babel-preset-expo" ],
  "env": {
    "development": {
      "plugins": [ "transform-react-jsx-source" ]
    }
  },
  "plugins": [
    [
      "module-resolver",
      {
        "root": [ "." ],
        "alias": {
          "^reactxx-mdi/(.+)": "reactxx-mdi/es",
          "reactxx-mdi": "reactxx-mdi/es/index",
          "reactxx-activable": "./deploy/es/activable",
          "reactxx-mediaq": "./deploy/es/mediaq",
          "reactxx-animation": "./deploy/es/animation",
          "reactxx": "./deploy/es/reactxx/native",
          "reactxx-basic": "./deploy/es/basic/native",
          "reactxx-mui": "./deploy/es/mui/native"
        }
      }
    ]
  ]
}
