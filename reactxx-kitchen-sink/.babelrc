{
  "presets": [ "babel-preset-expo" ],
  "env": {
    "development": {
      "plugins": [ "transform-react-jsx-source" ]
    }
  },
  "plugins": [
    [
      "module-resolver",
      {
        "root": [ "." ],
        "alias": {
          "^reactxx-mdi/(.+)": "reactxx-mdi/es/index",
          "reactxx-mdi": "reactxx-mdi/es/index",
          "reactxx-stateman": "reactxx-stateman/es/index",
          "reactxx-basic": "reactxx-basic/es/native/index",
          "reactxx": "reactxx/es/native/index",
          "reactxx-mui": "reactxx-mui/es/native"
        }
      }
    ]
  ]
}
