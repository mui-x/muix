{
  "compileOnSave": false,
  "compilerOptions": {
    "watch": true,
    "target": "es2017",
    "moduleResolution": "node",
    "module": "commonjs",
    "inlineSourceMap": true,
    "importHelpers": true,
    "strictNullChecks": false,
    "downlevelIteration": true,
    "strict": true,
    "strictFunctionTypes": false,
    "noImplicitAny": false,
    "outDir": "deploy",
    "resolveJsonModule": true,
    "lib": [
      "dom",
      "es2015",
      "esnext.asynciterable"
    ]
  },
  "exclude": [
    "material-design-icons-master/",
    "web/"
  ]
}